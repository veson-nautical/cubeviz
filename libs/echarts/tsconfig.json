{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "composite": false,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": false,
    "isolatedModules": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "strict": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "baseUrl": ".",
    "target": "ES2020",
    "lib": [
      "es2021"
    ],
    "rootDir": "./src",
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./src"
  ]
}
