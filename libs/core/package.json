{
  "name": "@cubeviz/core",
  "description": "React component library for cube",
  "version": "0.1.2",
  "license": "MIT",
  "homepage": "https://github.com/veson-nautical/cubeviz",
  "repository": "github:veson-nautical/cubeviz",
  "author": "Michael Shafir <mshafir@veson.com>",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup --dts",
    "dev": "tsup --watch",
    "prebuild": "barrelsby -d src -q -D",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "eslint src/**/*.ts* --fix"
  },
  "dependencies": {
    "@cubejs-client/core": "^0.30.64",
    "@cubejs-client/react": "^0.30.74",
    "lodash.isequal": "^4.5.0",
    "luxon": "^3.2.1",
    "numeral": "^2.0.6"
  },
  "devDependencies": {
    "@types/lodash.isequal": "^4.5.6",
    "@types/luxon": "^3.2.0",
    "@types/numeral": "^2.0.2",
    "barrelsby": "^2.5.1",
    "eslint": "^7.23.0",
    "eslint-config-next": "^12.0.8",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.28.0",
    "prettier": "^2.8.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "tsup": "^7.1.0",
    "typescript": "4.9.3"
  },
  "peerDependencies": {
    "react": "18",
    "react-dom": "18"
  }
}