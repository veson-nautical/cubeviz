{
  "name": "@cubeviz/table",
  "description": "React component library for cubejs - ag-grid table compoennts",
  "version": "0.1.1",
  "license": "MIT",
  "homepage": "https://github.com/veson-nautical/cubeviz",
  "repository": "github:veson-nautical/cubeviz",
  "author": "Michael Shafir <mshafir@veson.com>",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prebuild": "barrelsby -d src -q -D",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "eslint src/**/*.ts* --fix"
  },
  "dependencies": {
    "@blueprintjs/core": "^4.5.1",
    "@cubeviz/core": "workspace:*",
    "typed-data-table": "^1.2.4",
    "ag-grid-community": "^28.1.1",
    "ag-grid-react": "^28.1.1"
  },
  "devDependencies": {
    "barrelsby": "^2.5.1",
    "eslint": "^7.23.0",
    "eslint-config-next": "^12.0.8",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.28.0",
    "prettier": "^2.8.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "typescript": "4.9.3"
  },
  "peerDependencies": {
    "react": "18",
    "react-dom": "18"
  }
}